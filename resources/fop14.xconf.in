<?xml version="1.0"?>
<fop version="1.0">
  <base>.</base>
  <source-resolution>600</source-resolution>
  <target-resolution>600</target-resolution>
  
  <default-page-settings height="11.00in" width="8.50in"/>
  
  <renderers>
    <renderer mime="application/pdf">
      <filterList>
        <value>flate</value>
      </filterList>

      <fonts>
        <directory recursive="true">%FONT_PATH%</directory>
        <!-- <auto-detect/> -->
      </fonts>
    </renderer>
  </renderers>

</fop>
